{
  "name": "adamkoch.com",
  "version": "1.4.11",
  "description": "Adam Koch homepage",
  "author": "Adam Koch",
  "scripts": {
    "build": "mkdir -p dist && cp -v _redirects dist/ && cp -Rv src/assets/* dist/ && cp -Rv src/meta/* dist/ && parcel build ./src/index.pug './src/**/index.pug' ./src/404.pug --log-level verbose && node scripts/copy_font_files.mjs",
    "test:vue": "vue-cli-service test",
    "test:unit": "mocha",
    "test:system": "nightwatch tests/system/basic.js",
    "clean": "rm -rf ./dist/ && mkdir dist",
    "font": "rm -rf ./dist/ && mkdir dist && cp -v _redirects dist/ && cp -Rv src/assets/* dist/ && parcel build ./src/index.pug './src/**/index.pug' ./src/404.pug && cd dist && ../scripts/font.mjs && cd ..",
    "help": "echo \"npm run watch or npm run build\"",
    "postversion": "jq '.version' package.json | xargs -I{} tar -a -cf archive/{}.bz2 dist/* && git push && git push --tags",
    "test": "echo 'use test:unit' or 'test:system'",
    "updateTest": "SNAPSHOT_UPDATE=1 npm test",
    "watch": "mkdir -p dist && cp -v _redirects dist/ && cp -Rv src/assets/* dist/ && parcel ./src/index.pug './src/**/index.pug' ./src/404.pug --no-cache"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "blueimp-md5": "^2.18.0",
    "bootstrap": "^5.0.2",
    "countdown": "git://github.com/mckamey/countdownjs.git#1902fb23d21d84958b302c117fe472634a5bcf52",
    "jquery": "^3.6.0",
    "jquerykeyframes": "^1.0.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-countdown": "^0.0.3",
    "netlify": "^8.0.0",
    "server": "^1.0.36",
    "vue": "^3.2.4",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@parcel/core": "^2.0.0-rc.0",
    "@parcel/packager-raw-url": "v2.0.0-rc.0",
    "@parcel/reporter-build-metrics": "^2.0.0-rc.0",
    "@parcel/reporter-bundle-analyzer": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-jsonld": "^2.0.0-rc.0",
    "@parcel/transformer-pug": "v2.0.0-rc.0",
    "@parcel/transformer-sass": "v2.0.0-rc.0",
    "@parcel/transformer-vue": "v2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "v2.0.0-rc.0",
    "chai": "^4.3.4",
    "chromedriver": "^94.0.0",
    "glyphhanger": "^4.0.1",
    "highlight.js": "^11.2.0",
    "mocha": "^9.1.1",
    "nightwatch": "^1.7.11",
    "parcel": "^2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "snap-shot-it": "^7.9.6"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm-bundler.js"
  },
  "bugs": {
    "url": "https://github.com/aakoch/adamkoch.com/issues"
  },
  "homepage": "https://github.com/aakoch/adamkoch.com#readme",
  "keywords": [
    "adam",
    "koch",
    "software",
    "developer",
    "programmer"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aakoch/adamkoch.com.git"
  }
}

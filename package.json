{
  "name": "adamkoch.com",
  "version": "1.5.8",
  "description": "Adam Koch homepage",
  "author": "Adam Koch",
  "scripts": {
    "ng": "ng",
    "build": "./build.sh",
    "test:vue": "vue-cli-service test",
    "test:unit": "mocha",
    "test:system": "nightwatch tests/system/basic.js",
    "test:ng": "ng test",
    "clean": "rm -rf ./dist/ && mkdir dist;",
    "font": "rm -rf ./dist/ && mkdir dist && cp -v _redirects dist/ && cp -v _headers dist/ && cp -Rv src/assets/* dist/ && parcel build ./src/index.pug './src/**/index.pug' ./src/404.pug && cd dist && ../scripts/font.mjs && cd ..",
    "help": "echo \"npm run watch or npm run build\"",
    "postversion": "jq '.version' package.json | xargs -I{} tar -a -cf archive/{}.bz2 dist/* && git push && git push --tags",
    "serve": "npx serve dist/",
    "start": "ng serve",
    "test": "echo 'use test:unit' or 'test:system' or 'test:ng'",
    "updateTest": "SNAPSHOT_UPDATE=1 npm test",
    "watch": "mkdir -p dist && cp -v _redirects dist/ && cp -v _headers dist/ && cp -Rv src/assets/* dist/ && parcel ./src/index.pug './src/**/index.pug' ./src/404.pug --no-cache || ng build --watch --configuration development"
  },

{
  "name": "adamkoch.com",
  "version": "1.4.5",
  "description": "Adam Koch homepage",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "mkdir -p dist && cp -v _redirects dist/ && cp -Rv src/assets/* dist/ && parcel ./src/index.pug './src/**/index.pug' ./src/404.pug --no-cache",
    "build": "mkdir -p dist && cp -v _redirects dist/ && cp -Rv src/assets/* dist/ && parcel build ./src/index.pug './src/**/index.pug' ./src/404.pug",
    "clean": "rm -rf ./dist/ && mkdir dist",
    "postversion": "jq '.version' package.json | xargs -I{} tar -a -cf archive/{}.bz2 dist/* && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aakoch/adamkoch.com.git"
  },
  "keywords": [
    "adam",
    "koch",
    "software",
    "developer",
    "programmer"
  ],
  "author": "Adam Koch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aakoch/adamkoch.com/issues"
  },
  "homepage": "https://github.com/aakoch/adamkoch.com#readme",
  "devDependencies": {
    "@parcel/packager-raw-url": "v2.0.0-rc.0",
    "@parcel/reporter-build-metrics": "^2.0.0-rc.0",
    "@parcel/reporter-bundle-analyzer": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-pug": "v2.0.0-rc.0",
    "@parcel/transformer-sass": "v2.0.0-rc.0",
    "@parcel/transformer-vue": "v2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "v2.0.0-rc.0",
    "autoprefixer": "^10.3.4",
    "fontawesome-subset": "^2.0.0",
    "highlight.js": "^11.2.0",
    "parcel": "^2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "postcss": "^8.3.6",
    "postcss-import": "^14.0.2"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.esm-bundler.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@popperjs/core": "^2.9.2",
    "@vue/shared": "^3.2.11",
    "blueimp-md5": "^2.18.0",
    "bootstrap": "^5.0.2",
    "countdown": "git://github.com/mckamey/countdownjs.git#1902fb23d21d84958b302c117fe472634a5bcf52",
    "html2pug": "^4.0.0",
    "jquery": "^3.6.0",
    "jquerykeyframes": "^1.0.8",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "moment-countdown": "^0.0.3",
    "vue": "^3.2.4",
    "vue-router": "^4.0.11"
  }
}
